[flysight-manager]

storage_backend="dropbox"

[dropbox]
token="TOKEN_GOES_HERE"

[vimeo]
token="TOKEN_GOES_HERE"
enabled = "true"

[flysight]
enabled = "true"
mountpoint = "/mnt/flysight"
uuid = "false"

[gopro]
enabled = "true"

[gopro.video]
mountpoint="/mnt/gopro"
uuid = "false"

[gswoop]
enabled = "false"
binary = "/Applications/gSwoop.app/Contents/MacOS/gswoop"
