<!doctype html>
<html lang=en>
    <head>
        <meta charset=utf-8>
        <title>uploader report</title>
    </head>
    <body>
      Files uploaded:
      <ul>
      {% for file in files %}
      <li>{{file.fs_path}}</li>
      {% endfor %}
      </ul>

      {% if reason %}
      <div>
        <h3>Exit Reason</h3>
        {{ reason }}
      </div>
      {% endif %}

      <h3>Full log</h3>
      <pre>
{% for log in logs %}
{{ log }}{% endfor %}
      </pre>

    </body>
</html>

